<div class="col-md-6 col-md-offset-3">
<%= form_for @user do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :image %>
<%= f.text_field :image %>

<%= f.label :location %>
<%= f.text_field :location %>

<%= f.label :bio %>
<%= f.text_field :bio %>

<%= f.submit %>
<% end %>
</div>
